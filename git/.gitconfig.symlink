[alias]
    graph = log --decorate --oneline --graph
    whatsnew = !git log ..origin/`git rev-parse --abbrev-ref HEAD`
[color]
    ui = true
[help]
    autocorrect = 1
[core]
    excludesfile = ~/.gitignore_global
[include]
    path = ~/.gitconfig_user
    path = ~/.gitconfig_include
[init]
    templatedir = ~/.git_template

# vim: set ft=gitconfig :
