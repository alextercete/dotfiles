[core]
    autocrlf = true
    whitespace = cr-at-eol
    sshCommand = C:/Windows/System32/OpenSSH/ssh.exe
    pager = delta --diff-highlight
[interactive]
    diffFilter = delta --diff-highlight
[gpg "ssh"]
    program = "C:/Users/AlexTercete/AppData/Local/1Password/app/8/op-ssh-sign.exe"
[diff]
    tool = p4merge
[difftool]
    prompt = false
[merge]
    tool = p4merge
[mergetool]
    keepBackup = false
[mergetool "p4merge"]
    trustExitCode = true
